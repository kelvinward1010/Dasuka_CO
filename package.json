    "@emotion/styled": "^11.10.6",
    "antd": "^5.4.2",
    "axios": "^1.3.5",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "i18next": "^22.4.14",
    "i18next-browser-languagedetector": "^7.0.1",
    "immer": "^9.0.21",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-countup": "^6.4.2",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.10.0",
    "react-use": "^17.4.0",
    "read-excel-file": "^5.6.1",
